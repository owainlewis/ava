

newstack :: [A] -> Int -> Set[A]
newstack => { {} unstack }

define swons    { swap cons }
define unswons  { uncons swap }
define first    { unswons pop }
define rest     { uncons pop }
define i        { stack cdr swap infra unstack }
define dip      { stack rest rest swap infra cons unstack }
define not      { false true choice }
define square   { dup * }
define quote    { {} cons }
define rollup   { swap [swap] dip }

define inc      { 1 + }

